{
  "name": "lumenbox-client-web",
  "version": "0.1.0",
  "description": "LumenBox Web Client",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/lumenbox/lumenbox.git"
  },
  "author": "Garth Williams",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/lumenbox/lumenbox#readme",
  "scripts": {
    "prestart:css": "npm run build:css",
    "start:css": "npm run build:css -- --watch",
    "start:app": "cross-env NODE_PATH=src PORT=3000 BROWSER=none react-scripts start",
    "start": "npm-run-all --parallel start:*",
    "build:css": "node-sass-chokidar src/ -o src/",
    "build:app": "cross-env NODE_PATH=src react-scripts build",
    "build": "npm-run-all --serial build:*",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@cerebral/forms": "^3.1.1",
    "@cerebral/http": "^4.1.1",
    "@cerebral/react": "^3.1.1",
    "@cerebral/router": "^3.1.1",
    "bulma": "^0.6.2",
    "cerebral": "^4.1.1",
    "classnames": "^2.2.5",
    "core-js": "^2.5.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "react-scripts": "1.1.1"
  }
}
